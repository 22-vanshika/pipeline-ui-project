{
    "builds": [
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend-vite/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/main.py"
      },
      {
        "src": "/(.*)",
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/index.html"
      }
    ]
  }